package com.nttdata;

public class Temperatura {
	
	private String formato;
	
	public Temperatura(String formato) {
		this.formato = formato;
	}
	
	public double celciusToFahr(double temp) {
		return (temp - 32) / 1.8;
	}
	
	public double farhToCelsius(double temp) {
		return (temp * 1.8) + 32;
	}
	
}
